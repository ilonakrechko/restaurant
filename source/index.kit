                        <a href="" class="btn history__btn">Read more</a>
                    </div>
                </div>
                <div class="col-lg-3">
                    <div class="food-images food-images-down">
                        <img src="img/food1.jpg" alt="Pasta" class="food-images__item">
                        <img src="img/food2.jpg" alt="Chicken" class="food-images__item">
                    </div>
                </div>
                <div class="col-lg-3">
                    <div class="food-images">
                        <img src="img/food3.jpg" alt="Meat" class="food-images__item">
                        <img src="img/food4.jpg" alt="Fish" class="food-images__item">
                    </div>
                </div>
            </div>
        </div>
    </section>

    <section class="special">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <h2 class="special-title">
                        Today Special
                    </h2>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-6">
                    <div class="food-card">
                        <img src="img/dish1.jpg" alt="Dish 1" class="food-card__img img-fluid">
